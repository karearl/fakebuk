<div>
    {{#each buttons as |button|}}
    <button>
        <svg aria-label="{{button.label}}" fill="none" stroke="currentColor" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg">
            <path strokeLinecap="round" strokeLinejoin="round" strokeWidth={2} d="{{button.path}}"></path>
        </svg>
    </button>
    {{/each}}
</div>